extends /layout

block content
	section#hero.cover.height-60.imagebg.parallax.text-center(data-overlay="5")
		//- background image
		.background-image-holder
			//-background image for smaller devices
			img(src="/img/header/calculators.jpg" alt="Header Image")
		div#hero-content.container.pos-vertical-center
	section.unpad--bottom
		.container
			.row
				.col-sm-12
					h2.color--blue Calculators
	section
		.container
			.row
				.col-sm-12
					h5.color--blue Budget Calculator
					p.
						Need some help putting together a budget? We've got you covered.
				.col-sm-12
					a.btn.btn-blue(href="https://www.quicken.com/budget-calculator" target="_blank")
						span.btn__text Budget Calculator
	section
		.container
			.row
				.col-sm-12
					h5.color--blue Home Loan Calculator
					p.
						How much of a mortgage can you afford? What other fees should I budget for? Let us help you figure it out.
				//-.col-sm-12
					a.btn.btn-blue(href="https://tularefcu.cudlautosmart.com/Finance" target="_blank" rel="noopener") 
						span.btn__text Loan Calculator
