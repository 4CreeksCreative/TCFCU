extends /layout

block content
	section#hero.cover.height-60.imagebg.parallax.text-center
		if contentful.savings[0].headerPosition != undefined
			.background-image-holder(class="#{contentful.savings[0].headerPosition}")
				if contentful.savings[0].headerImage != undefined
					img(src="#{asset(contentful.savings[0].headerImage)}" alt="header image")
		else
			.background-image-holder.bg-cc
				if contentful.savings[0].headerImage != undefined
					img(src="#{asset(contentful.savings[0].headerImage)}" alt="header image")
				else
					img(src="/img/header/savings.jpg" alt="header image")
	section.unpad--bottom
		.container
			.row
				.col-sm-12
					//-p= JSON.stringify(contentful.savings)
					h2.color--blue= contentful.savings[0].pageTitle
	section
		.container
			if contentful.savings[0].subPageSections != undefined
				for item in contentful.savings[0].subPageSections
					.row.mb--2
						.col-sm-12
							if item.fields.subhead != undefined
								h5.color--blue= item.fields.subhead
							if item.fields.body != undefined
								.md!= md(item.fields.body)
